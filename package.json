{
  "name": "html-analytics",
  "version": "0.0.1",
  "description": "html analytic tools",
  "main": "index.ts",
  "scripts": {
    "css-selector-explorer": "tsc Proof-of-Concepts/CSSParsing/selectorExplorer.ts && browserify Proof-of-Concepts/CSSParsing/selectorExplorer.js -o Proof-of-Concepts/bundle.js",
    "html-class-counter": "tsc Proof-of-Concepts/HTMLParsing/classCounter.ts && browserify Proof-of-Concepts/HTMLParsing/classCounter.js -o Proof-of-Concepts/bundle.js",
    "html-id-analyser": "tsc Proof-of-Concepts/HTMLParsing/idAnalyser.ts && browserify Proof-of-Concepts/HTMLParsing/idAnalyser.js -o Proof-of-Concepts/bundle.js",
    "html-style-class-counter": "tsc Proof-of-Concepts/HTMLParsing/styleCounter.ts && browserify Proof-of-Concepts/HTMLParsing/styleCounter.js -o Proof-of-Concepts/bundle.js"
  },
  "dependencies": {
    "cssom": "^0.3.6",
    "htmlparser2": "^3.10.1",
    "lint-staged": "^8.1.5",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "@types/node": "^11.11.3",
    "husky": "^1.3.1",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoshMelton/HTMLAnalytics.git"
  },
  "keywords": [
    "html",
    "analytics"
  ],
  "author": "Joshua Melton",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/JoshMelton/HTMLAnalytics/issues"
  },
  "homepage": "https://github.com/JoshMelton/HTMLAnalytics#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix --project .",
      "git add"
    ]
  }
}
